@model IEnumerable<Lucky.Models.WinnerList>

@{
    ViewBag.Title = "Result";
}

<h2>Result</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PrizeType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WinningNumber)
        </th>
        <th></th>
    </tr>
@if (Model.Count() > 0)
{
foreach (var item in Model)
{
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PrizeType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WinningNumber)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Details", "Details", new { id = item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.Id })
        </td>
    </tr>
}
}
else
{
    <tr>
        <td colspan="4" class="text-center">No Data</td>
    </tr>
}
</table>
